.wwp-container,
.wwp-container--full {
  overflow: auto;
}

.wwp-root h2 {
  font-size: var(--font-size-2);
}

img {
  max-width: 100%;
  align-self: center;
}

.wwp-container--full {
  width: 100%;
  padding-left: 0;
  padding-right: 0;
}

.wwp-container--alt-bg {
  background-color: #f5f5f5;
  margin-bottom: 20px;
}

.wwp-hero {
  width: 100%;
  padding-top: 31%;
  background-size: cover;
  background-position: center;
  position: relative;
  min-height: 170px;
}

.wwp-hero__content {
  position: absolute;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  background: rgba(51, 51, 51, 0.2);
}

.wwp-root .text-center p {
  margin-left: auto;
  margin-right: auto;
}

.wwp-root p.wwp-p--big {
  font-size: 22px;
  max-width: 28em;
}

.text-center .wwp-p--big {
  margin-left: auto;
  margin-right: auto;
}

h1.wwp-page-title {
  color: white;
  font-size: 46px;
  line-height: 50px;
  text-transform: uppercase;
  font-weight: 700;
}

h1.wwp-page-title.wwp-page-title--script {
  font-family: "WFHavanaSunset";
  text-transform: none;
  letter-spacing: 0;
  font-weight: normal;
  font-size: 121px;
}

h2.wmp-page-tagline {
  color: white;
  font-weight: normal;
  font-size: 28px;
  line-height: 50px;
}

@media screen and (max-width: 425px) {
  h1.wwp-page-title {
    font-size: 18px;
    line-height: 18px;
  }

  h1.wwp-page-title.wwp-page-title--script {
    font-size: 60px;
  }

  h2.wmp-page-tagline {
    font-size: 15px;
    line-height: 18px;
  }
}
.wwp-line-after:after {
  content: "";
  display: block;
  height: 5px;
  width: 68px;
  background: #7dcfb6;
  margin: 30px auto;
}

.wwp-line-after--taller:after {
  margin: 50px auto;
}

.wwp-section--b {
  background: #f5f5f5;
  padding: 60px 40px;
  margin-top: 40px;
}
@media screen and (max-width: 767px) {
  .wwp-section--b {
    padding: 30px 10px;
  }
}

.wwp-section--line-after:after {
  content: "";
  display: block;
  width: 100%;
  height: 1px;
  background: #e5e5e5;
  margin-top: 76px;
  margin-bottom: 76px;
}

#mission_about {
  margin-top: 76px;
}

.wwp-area {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  align-items: center;
  margin-top: 60px;
}

.wwp-area__half {
  width: 50%;
}

.wwp-root h4:not(.wwp-team-member__title) {
  font-size: 20px;
  line-height: 1.3;
  max-width: 18em;
}

.wwp-root .text-center h4 {
  margin-left: auto;
  margin-right: auto;
}

.wwp-m-padded {
  padding: 40px;
}

@media (max-width: 800px) {
  .wwp-section--a .wwp-area {
    flex-direction: column;
  }

  .wwp-area__half {
    width: 100%;
  }

  .wwp-section--a .responsive-image {
    max-width: 300px;
    height: auto;
    margin-left: auto;
    margin-right: auto;
  }

  .wwp-section--a .wwp-m-padded {
    padding: 40px 0;
  }

  .wwp-section--a p {
    max-width: 28em;
    text-align: center;
    margin-left: auto;
    margin-right: auto;
  }

  .wwp-section--a .cta {
    text-align: center;
  }

  .wwp-email {
    text-align: center;
  }
}
.wwp-img-full {
  width: 100%;
  height: 100%;
  object-fit: cover;
}

#giving_back {
  margin-bottom: 80px;
}

.wwp-feedback {
  margin-top: 50px;
}

.wwp-feedback-item__content__name {
  margin: 30px auto;
}

.wwp-feedback-item__content__copy > p {
  max-width: 38em;
}

.wwp-feedback-item__picture .responsive-image {
  max-width: 165px;
  max-height: 165px;
  margin-left: auto;
  margin-right: auto;
}

.wwp-feedback-item__picture .responsive-image__img {
  border-radius: 50%;
}

#feedback .slick-dots {
  display: block !important;
}

#feedback .slick-dots li button:before {
  font-size: 9px;
  opacity: 0.1;
}

#feedback .slick-dots li.slick-active button:before {
  opacity: 0.5;
}

svg.placeholder {
  opacity: 0.3;
  background: #dcdcdc;
  width: 100%;
  height: 100%;
}

.placeholder--user {
  border-radius: 50%;
}

.wwp-email {
  margin: 15px auto;
}

.wwp-email__envelope > svg {
  height: 2em;
  margin-bottom: -0.667em;
  margin-right: 0.25em;
  fill: currentColor;
}

.wwp-email a span {
  color: inherit;
}

.wwp-email a:hover span {
  color: inherit;
}

.cta {
  margin-top: 30px;
}

@media screen and (max-width: 767px) {
  .sm--order-1 {
    order: 1;
  }

  .sm--order-2 {
    order: 2;
  }
}
.wwp-comparison-graph-container {
  width: 220px;
  margin-bottom: 1.5em;
}

.wwp-comparison-graph {
  position: relative;
  width: 100%;
  padding-top: 100%;
}

.wwp-comparison-graph__bg {
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  background-size: contain;
  background-position: center;
  background-repeat: no-repeat;
}

.wwp-comparison-graph__bg--wmp {
  background-image: url("{{ 'twenty.png' | asset_url }}");
}

.wwp-comparison-graph__bg--others {
  background-image: url("{{ 'eighty.png' | asset_url }}");
}

.wwp-comparison-graph__content {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.wwp-comparison-graph__content > p {
  line-height: 1;
}

.wwp-comparison-graph__label {
  margin: 1em auto 0.75em;
}

.wwp-comparison-graph__frame {
  width: 96px;
  margin-left: auto;
  margin-right: auto;
}

.wwp-comparison-graph__frame > img {
  max-width: 100%;
  height: auto;
}

.wwp-comparison-graph__frame--others > img {
  opacity: 0.7;
}

.wwp-comparison-graph__cost {
  margin-top: 0;
  font-size: 1.66em;
}

.wwp-comparison-graph__cost--others {
  color: #f24640;
}

.wwp-comparison-graph__cost--wmp {
  color: #39d055;
}

.wwp-comparison-list ul {
  list-style: none;
  padding-left: 0;
}

.wwp-comparison-list li {
  position: relative;
  padding-left: 2.5em;
  line-height: 1.2;
  padding-top: 1em;
  padding-bottom: 1em;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.wwp-comparison-list li:before {
  position: absolute;
  content: "";
  display: block;
  left: 0;
  background-image: url("{{ 'wwp-check.svg' | asset_url }}");
  background-position: center;
  background-size: contain;
  background-repeat: no-repeat;
  width: 1.75em;
  height: 1.75em;
}

.wwp-comparison-list li.wwp-bullet--minus:before {
  background-image: url("{{ 'wwp-minus.svg' | asset_url }}");
}

@media screen and (min-width: 800px) {
  .desktop--wwp-area__half--comparison--right .wwp-comparison-graph-container {
    margin-left: auto;
  }
  .desktop--wwp-area__half--comparison--right .wwp-comparison-list ul {
    text-align: right;
  }
  .desktop--wwp-area__half--comparison--right .wwp-comparison-list li {
    padding-left: 0;
    padding-right: 2.5em;
    justify-content: flex-end;
  }
  .desktop--wwp-area__half--comparison--right .wwp-comparison-list li:before {
    left: auto;
    right: 0;
  }
}
